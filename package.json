{
  "name": "diffyjs",
  "version": "1.0.0",
  "description": "A client-side JS motion detection library",
  "main": "dist/lib.js",
  "scripts": {
    "start": "npm-run-all --parallel open:dev lint:watch",
    "demo": "npm-run-all --parallel open:demo lint:watch",
    "open:dev": "node_modules/.bin/babel-node tools/dev-server.js",
    "open:demo": "node_modules/.bin/babel-node tools/demo-server.js",
    "build": "node -r babel-register node_modules/.bin/webpack",
    "build:lib": "npm run build -- --config webpack.config.js",
    "build:demo": "npm run build -- --config webpack.config.demo.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools demo/src",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/test-setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maniart/diffyjs.git"
  },
  "author": "Mani Nilchiani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maniart/diffyjs/issues"
  },
  "homepage": "https://github.com/maniart/diffyjs#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "npm-run-all": "^3.1.2",
    "open": "0.0.5",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "readable-stream": "^2.2.2"
  }
}
